{
  "containerDefinitions": [
    {
      "name": "swagger",
      "image": "%s.dkr.ecr.us-east-1.amazonaws.com/my-dev-space-swagger:staging",
      "essential": true,

      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "my-dev-space-swagger-stage",
          "awslogs-region": "us-east-1"
        }
      },
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "URL",
          "value": "swagger.json"
        }
      ],
      "memoryReservation": 300
    }
  ],
  "family": "my-dev-space-swagger-stage-td"
}
